{
  "name": "vizceral-example",
  "version": "0.0.1",
  "description": "Vizceral example app",
  "author": "Justin Reynolds <jreynolds@netflix.com>",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint src --ext .js --ext .jsx",
    "copy:fonts": "cpx \"./node_modules/source-sans-pro/**/*\" ./dist/fonts",
    "copy:json": "cpx \"./src/*.json\" ./dist",
    "dev-server": "webpack-dev-server --content-base dist/ --history-api-fallback",
    "dev": "npm-run-all clean --parallel copy:* dev-server",
    "validate": "npm ls"
  },
  "engines": {
    "node": "~4.4.0"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "flux": "^2.1.1",
    "hammerjs": "^2.0.8",
    "immutable": "^3.8.1",
    "jquery": "^2.2.3",
    "keymirror": "^0.1.1",
    "keypress.js": "^2.1.0-1",
    "lodash": "^4.12.0",
    "numeral": "^1.5.3",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "request": "^2.72.0",
    "socket.io-client": "^1.4.6",
    "source-sans-pro": "^2.0.10",
    "superagent": "^2.0.0",
    "tween.js": "^16.3.4",
    "vizceral-react": "^1.0.16",
    "webfontloader": "^1.6.24"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.x",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^2.1.1",
    "precommit-hook": "^3.0.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Netflix/vizceral.git"
  },
  "pre-commit": [
    "lint"
  ]
}
